<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white">

    <TextView
        android:id="@+id/developer_info_text"
        android:layout_width="329dp"
        android:layout_height="517dp"
        android:text="text"
        android:textColor="@color/black"
        android:paddingTop="55sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.453" />

    <TextView
        android:id="@+id/developer_info_label"
        android:layout_width="327dp"
        android:layout_height="50dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="2dp"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@color/black"
        android:background="@color/login_label_color"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/developer_info_text"
        app:layout_constraintEnd_toEndOf="@+id/developer_info_text"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/developer_info_text"
        app:layout_constraintTop_toTopOf="@+id/developer_info_text"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>